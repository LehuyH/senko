{
    "extends": "airbnb-typescript-prettier",
    "rules": {
        "import/extensions": "off",
        "no-console": "off",
        "no-await-in-loop": "off",
        "no-restricted-syntax": "off",
        "@typescript-eslint/explicit-module-boundary-types": "off"
    },
    "ignorePatterns": [
        "rollup.config.js",
        "dist/*",
        "/**/*.d.ts"
    ]
}